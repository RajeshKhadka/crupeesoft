{% extends 'base.twig' %}
{% block content %}
        <!-- Banner Section -->
        <section class="banner-section banner-one banner-one-page" id="home">

            <div class="banner-carousel owl-theme owl-carousel">
            {% for item in sliders %}
                <!-- Slide Item -->
                <div class="slide-item">
                    <div class="image-layer" style="background-image: url({{item.thumbnail.src}});"></div>

                    <div class="auto-container">
                        <div class="content-box">
                            <div class="content">
                                <div class="inner text-center">
                                    <div class="sub-title">{{item.post_title}}</div>
                                    {# <h1>Discover <br> <span>Team Crupee</span></h1> #}
                                    <h1><span>{{item.post_content}}</span></h1>
                                    <div class="link-box">
                                        <a class="theme-btn btn-style-one" href="javascript:void(0)">
                                            <i class="btn-curve"></i>
                                            <span class="btn-title">Discover More</span>
                                        </a>
                                        <div class="vid-link">
                                            <a href="{{item.meta('video_url')}}"
                                                class="lightbox-image">
                                                <div class="icon"><span class="flaticon-play-button-1"></span><i
                                                        class="ripple"></i></div>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
{% endfor %}

            </div>
        </section>
        <!--End Banner Section -->

        <!--About Section-->
        <section class="about-section" id="about">
            <div class="auto-container">
                <div class="row clearfix">
                    {% set aboutGroup = post.meta('about_us_section') %}
                    <!--Image Column-->
                    {% set delay = 0 %}
                    <div class="image-column col-xl-6 col-lg-12 col-md-12 col-sm-12">
                        <div class="inner">
                        {% for key,item in aboutGroup.image_field %}
                            {% if (key+1)%2 == 0 %}
                                {% set var = 'fadeInLeft' %}

                            {% else %}
                                {% set var = 'fadeInUp' %}
                            {% endif %}
                            <div class="image-block wow {{var}}" data-wow-delay="{{delay}}ms" data-wow-duration="1500ms"><img src="{{Image(item.image).src}}" alt="image"></div>
                            {# <div class="image-block wow fadeInUp" data-wow-delay="300ms" data-wow-duration="1500ms"><img
                                    src="{{site.theme.link}}/assets/images/resource/featured-image-2.jpg" alt=""></div> #}
                         {% set delay = delay+300 %}
                            {% endfor %}
                         </div>
                    </div>
                    <!--Text Column-->
                    <div class="text-column col-xl-6 col-lg-12 col-md-12 col-sm-12">
                        <div class="inner">
                            <div class="sec-title">
                                <h2>{{aboutGroup.section_title}}<span class="dot">.</span></h2>
                                <div class="lower-text">{{aboutGroup.sub_title_of_the_section}}</div>
                            </div>
                            <div class="text">
                                <p>{{aboutGroup.small_description}}</p>
                            </div>
                            <div class="text clearfix">
                                <ul>
                                {% for item in aboutGroup.list_section %}
